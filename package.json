{
  "name": "tpl-react",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "description": "Frontend starter with React, Redux, Webpack and friends",
  "homepage": "https://github.com/andreidmt/tpl-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreidmt/tpl-react.git"
  },
  "bugs": {
    "url": "https://github.com/andreidmt/tpl-react/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidmt"
  },
  "scripts": {
    "----UTIL": "",
    "setup:clean": "rm -rf ./node_modules",
    "setup": "npm run setup:clean && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update",
    "update-force": "npm-check --update-all",
    "----DEV": "",
    "start": "webpack serve --mode development",
    "----BUILD": "",
    "build:clean": "rm -rf ./dist",
    "build": "npm run build:clean && webpack --mode production",
    "----LINT": "",
    "lint:css": "stylelint 'src/*.css' 'src/**/*.css'",
    "lint:md": "markdownlint *.md",
    "lint:js": "eslint --ext .jsx,.js,.json --quiet src",
    "lint": "npm run lint:md && npm run lint:css && npm run lint:js",
    "----TEST": "",
    "test": "riteway -r @babel/register -r ignore-styles 'src/**/*.test.js' 'src/**/*.test.jsx' | tap-nirvana",
    "tdd": "nodemon -e js,jsx,json --watch src --exec 'npm test'"
  },
  "dependencies": {
    "@asd14/fetch-browser": "^2.0.0",
    "@asd14/m": "^5.0.0",
    "@asd14/react-hooks": "^1.0.0",
    "@asd14/state-list": "^7.8.1",
    "@babel/runtime": "^7.13.8",
    "classnames": "^2.2.6",
    "cuid": "^2.1.8",
    "debug": "^4.1.1",
    "luxon": "^1.26.0",
    "qs": "^6.9.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "sanitize.css": "^12.0.1",
    "socket.io-client": "^3.1.2"
  },
  "devDependencies": {
    "@asd14/eslint-config": "^5.29.0",
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-react-constant-elements": "^7.12.13",
    "@babel/plugin-transform-react-display-name": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
